{
  "name": "tiny-project",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "start": "vite --jsx-factory=h",
    "build": "parcel build index.html",
    "jest": "jest --collect-coverage",
    "cypress": "cypress open",
    "cypress:run": "start-server-and-test 'start' 3000 'cypress run'",
    "bundlesize": "npm run build && bundlesize -f 'dist/src.*.js' -s 7.8kB",
    "lighthouse": "start-server-and-test 'start' 3000 'lighthouse http://localhost:3000 --view --output-path=./coverage/lighthouse.json'",
    "format": "prettier --write ."
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run jest && pretty-quick --staged",
      "pre-push": "npm run bundlesize && npm run cypress:run"
    }
  },
  "dependencies": {
    "preact": "^10.4.1",
    "preact-router": "^3.2.1"
  },
  "devDependencies": {
    "@babel/core": "^7.9.6",
    "@babel/preset-env": "^7.9.6",
    "@types/enzyme": "^3.10.5",
    "@types/jest": "^25.2.1",
    "babel-jest": "^26.0.1",
    "bundlesize": "^0.18.0",
    "cypress": "^4.11.0",
    "enzyme": "^3.11.0",
    "enzyme-adapter-preact-pure": "^2.2.0",
    "enzyme-to-json": "^3.4.4",
    "husky": "^4.2.5",
    "jest": "^26.0.1",
    "lighthouse": "^6.1.1",
    "parcel-bundler": "^1.12.4",
    "parcel-plugin-bundle-visualiser": "^1.2.0",
    "parcel-plugin-clean-dist": "^0.0.6",
    "parcel-plugin-static-files-copy": "^2.3.1",
    "prettier": "^2.0.5",
    "pretty-quick": "^2.0.1",
    "start-server-and-test": "^1.11.2",
    "ts-jest": "^25.5.1",
    "typescript": "^3.8.3",
    "vite": "^2.9.13"
  },
  "staticFiles": {
    "staticPath": [
      {
        "staticPath": "static",
        "staticOutDir": "static"
      }
    ]
  }
}
