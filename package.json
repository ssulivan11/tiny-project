    "preact-router": "^3.2.1"
  },
  "devDependencies": {
    "@babel/core": "^7.9.6",
    "@babel/preset-env": "^7.9.6",
    "@types/enzyme": "^3.10.5",
    "@types/jest": "^25.2.1",
    "babel-jest": "^26.0.1",
    "cypress": "^4.5.0",
    "enzyme": "^3.11.0",
    "enzyme-adapter-preact-pure": "^2.2.0",
    "enzyme-to-json": "^3.4.4",
    "husky": "^4.2.5",
    "jest": "^26.0.1",
    "parcel-bundler": "^1.12.4",
    "prettier": "^2.0.5",
    "pretty-quick": "^2.0.1",
    "start-server-and-test": "^1.11.0",
    "ts-jest": "^25.5.1",
